<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Background berwarna biru -->
    <solid android:color="@color/blue5" /> <!-- Warna biru, bisa diganti sesuai kebutuhan -->

    <!-- Sudut membulat -->
    <corners android:radius="8dp" /> <!-- Radius sudut rounded, bisa diubah sesuai keinginan -->

    <!-- Padding untuk konten di dalam -->
    <padding
        android:left="8dp"
        android:top="8dp"
        android:right="8dp"
        android:bottom="8dp" />
</shape>

